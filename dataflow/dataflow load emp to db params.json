{
	"name": "dataflow load emp to db params",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmpMaster",
						"type": "DatasetReference"
					},
					"name": "empMasterLake"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dbo_NYC_Payroll_EMP_MD",
						"type": "DatasetReference"
					},
					"name": "empMasterDb"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     EmpID_param as integer (100002)",
				"}",
				"source(output(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> empMasterLake",
				"empMasterLake filter(toInteger(EmployeeID)!=$EmpID_param) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> empMasterDb"
			]
		}
	}
}